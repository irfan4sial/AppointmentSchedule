<h3>New Doctor</h3>

<%= form_for @doctor do |f| %>
  <div>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div>
    <%= f.label :phone_number %>
    <%= f.text_field :phone_number %>
  </div>
  <div>
    <%= f.label :specialization %>
    <%= f.text_field :specialization %>
  </div>
  <div>
    <%= f.submit %>
  </div>
<% end %>